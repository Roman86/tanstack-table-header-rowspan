{
  "compilerOptions": {
    "target": "ESNext",
    "module": "Node16",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "allowJs": true,
    "skipLibCheck": true,
    "outDir": "dist"
  },
  "include": ["./"],
  "exclude": ["node_modules", "dist"]
}
